pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                 sh 'echo Building'
            }
        }
            stage('Package') {
            steps {
                sh 'echo Packaging'
            }
        }
            stage('Deploy') {
            steps {
                sh 'echo Deploying'
            }
        }
            stage('artifact') {
            steps {
                sh 'echo Create artifact'
            }
        }
    }
}
