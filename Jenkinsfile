pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                 echo 'Building'
            }
        }
            stage('Test') {
            steps {
                echo 'Testing'
            }
        }
            stage('Deploy') {
            steps {
                echo 'Hello World'
            }
        }
            stage('Deliver') {
            steps {
                echo 'Delivering'
            }
        }
    }
}
